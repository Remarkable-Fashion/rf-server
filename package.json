{
    "name": "rf-server",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepare": "husky install",
        "build": "npx ttsc",
        "start": "node dist/index.js",
        "start:dev": "node dist/index.js",
        "start:w": "nodemon --watch src/** --ext ts,json --ignore src/**/*.spec.ts --exec ts-node ./src/index.ts",
        "lint": "eslint ./src",
        "lint:es:fix": "eslint --ext js --ext ts './' --fix",
        "prettier": "prettier --write src/**/*.ts",
        "prettier:fix": "prettier --write src/**/*.ts --fix",
        "lint-staged": "lint-staged",
        "db:generate": "npx prisma generate",
        "db:migrate": "npx prisma migrate dev",
        "db:generate:dev": "dotenv -e .env.dev npx prisma generate",
        "db:migrate:dev": "dotenv -e .env.dev npx prisma migrate dev",
        "db:push": "npx prisma db push",
        "seed": "dotenv -e ./.env ts-node ./src/db/seeds/seed.ts",
        "seed:dev": "dotenv -e ./.env.dev ts-node ./src/db/seeds/seed.ts",
        "cron": "node ./dist/cron.js"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Remarkable-Fashion/rf-server.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Remarkable-Fashion/rf-server/issues"
    },
    "homepage": "https://github.com/Remarkable-Fashion/rf-server#readme",
    "devDependencies": {
        "@types/connect-flash": "^0.0.37",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.11.9",
        "@types/node-cron": "^3.0.7",
        "@types/passport": "^1.0.11",
        "@types/passport-kakao": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "dotenv-cli": "^7.2.1",
        "eslint": "^8.25.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "prisma": "^4.16.2",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.17.11-patch.1",
        "@prisma/client": "^4.16.2",
        "axios": "^1.1.3",
        "connect-flash": "^0.1.1",
        "connect-redis": "^7.1.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-session": "^1.17.3",
        "jsonwebtoken": "^8.5.1",
        "moment-timezone": "^0.5.43",
        "mongodb": "^5.4.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.2",
        "passport": "^0.6.0",
        "passport-kakao": "^1.0.1",
        "rate-limit-redis": "^3.0.1",
        "redis": "^4.5.0",
        "ttypescript": "^1.5.13",
        "typia": "^3.6.6",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    }
}
